import java.util.Scanner;
public class Sequence
{
    public static void main(String[] args)
    {
    	Scanner scan = new Scanner(System.in);
    	int[] sequence = new int[10];
       System.out.println("Vuvedi 10 chisla:");
        for (int i = 0; i < sequence.length; i++)
        {
        	sequence[i] = scan.nextInt();
        }
        scan.close();
    }
    public static void arr(int[] array,int[] sequence)
    {
        int idex = 0;
        int previuslen = array[0];
        int start= 0 ;
        int[] temporaryArray = new int[array.length];
        int ElementCounter = 0;
        int OldElementCounter = 0;
      for(int j=0;j<sequence.length;j++){
            if (previuslen < array[idex + 1])
            {
                start = idex + 1;
                ElementCounter++;
                previuslen=array[idex+1];

            }
            else if (array[idex] >= array[idex + 1])
            {
                previuslen = array[idex + 1];
                if (ElementCounter > OldElementCounter)
                {
                    for (int j = 0; j < ElementCounter+1; j++)
                    {
                        temporaryArray[j] = array[k - ElementCounter];
                        k++;
                    }
                    OldElementCounter = ElementCounter;
                }
                ElementCounter = 0;
            }
            i++;
      }
        }
        for (int z = 0; z < OldElementCounter+1; z++)
        {
            System.out.printf("%d ",temporaryArray[z]);
        }
    }
}