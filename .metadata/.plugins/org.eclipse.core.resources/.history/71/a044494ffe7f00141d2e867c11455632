package domashnarabota2;

public class ZadachaKarti {

	public static void main(String[] args) {
		String[] karti = {"A","K","Q","J","10","9","8","7","6","5","4","3"};
		String[] ime = {"spatiq ","karo","kupa","pika"};

		for(int g=0;g<=3;g++){
	System.out.printf("%s\t ",ime[g]);	
	}
		
	for(int i=0;i<=karti.length;i++)
	{		System.out.println("");
			
		for(int j=1;j<=ime.length;j++)
		{	
			System.out.printf("%s \t ",karti[i]);
			
		}
		
		
	}
		
		
	}
	}