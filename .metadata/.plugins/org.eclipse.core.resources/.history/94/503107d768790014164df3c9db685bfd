package pravougulnik;

public class razmqna {

	public static void main(String[] args) {
		int a=1;
		int b=3;
		int temp;
		//temp=b;
		//b=a;
		//a=temp;
		a = a+ b; //now a is 30 and b is 20
		b = a -b; //now a is 30 but b is 10 (original value of a)
		a = a -b; //now a is 20 and b is 10, numbers are swapped		
		System.out.println("tova e a:" +a);
		System.out.println("tova e b:"+b);
//Дефинирайте две int променливи със стойности съответно 1 и 3.
//Опитайте се да размените техните стойности.

	}

}
