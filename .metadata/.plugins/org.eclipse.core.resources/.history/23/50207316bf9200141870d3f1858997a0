
public class Sequence {

	public static void main(String[] args) {
		int j=0, cou=0, max=0,n=0;  //c = final array; will contain the longest consecutive, increasing sequence of numbers
		int [] c= new int[10];
		int [] a= {1, 3, 5, 1, 5, 7, 8, 9, 10, 11, 12};
		int i,k,z = 0;
		for ( k=0,i = 0; i<a.length; ++i)
		{if(a[i+1] > a[i])
		  {  ++cou;
		    if (cou>max)
		        {max = cou;
		        z=k;
		        }

		   }
		    else
		        {
		            k=i+1;
		            cou = 0;
		         j=0;}
		}
		for( j = z; j <a.length ; ++j)
		if(a[j]<a[j+1])
		System.out.printf("%d ",a[j]);
		else
		 break;
		System.out.printf("%d",a[j]);

	}

}
